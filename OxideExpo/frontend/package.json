{
  "name": "empleos-inclusivos-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-not-root": "node -e \"if(process.getuid && process.getuid()===0){console.error('\\x1b[31mError: Do not run npm as root. This causes permission issues with .next folder.\\x1b[0m');process.exit(1)}\"",
    "preinstall": "npm run check-not-root",
    "predev": "npm run check-not-root",
    "prebuild": "npm run check-not-root",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-gen": "cd ../backend && cargo test --lib -- --nocapture",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "next": "^14.2.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "pg": "^8.18.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
